    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "James Willans",
            "email": "kyoushu@gmail.com"
        }
    ],
    "require": {
        "php": "~7.1",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Kyoushu\\ProgressTracker\\": "src/"
        }
